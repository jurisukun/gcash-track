{
  "expo": {
    "name": "gcash-tracker",
    "slug": "gcash-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/finalicons/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "permissions": [
            "CAMERA",
            "READ_EXTERNAL_STORAGE",
            "WRITE_EXTERNAL_STORAGE"
          ]
        }
      ]
    ],
    "splash": {
      "image": "./assets/finalicons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4F4FF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/finalicons/adaptive-icon.png",
        "backgroundColor": "#F4F4FF"
      },
      "package": "com.jurisu_kun.gcashtracker",
      "permissions": [
        "android.permission.NETWORK_ACCESS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": { "babel": { "include": ["@ui-kitten/components"] } }
    },
    "extra": {
      "eas": {
        "projectId": "917a255b-1227-426f-a459-8f4c16358f00"
      }
    }
  }
}
